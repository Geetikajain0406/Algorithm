#include <iostream>
using namespace std;
void permute(string &s,int l,int r){
    if(l==r){
        cout<<s<<" ";
        return;
    }
    for(int i=l;i<r;i++){
        swap(s[l],s[i]);
        permute(s,l+1,r);
       swap(s[l],s[i]);
    }
}
int main() {
     string M = "ABC";
     int s=0;
     int e=M.size();
   permute(M,s,e);
  
    return 0;
}
