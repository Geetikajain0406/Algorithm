#include <iostream>
using namespace std;
#define n 4
int check[n][n];
bool isitsafe(int row,int col){
    for(int i=0;i<row;i++){
        if(check[i][col]){
            return false;
        }
    }
    for(int i=row,j=col;i>=0 && j>=0;i--,j--){
        if(check[i][j]){
            return false;
        }
    }
     for(int i=row,j=col;i>=0 && j<n;i--,j--){
        if(check[i][j]){
            return false;
        }
    }
    return true;
}
bool solvequeen(int row){
    if(row==n){
        return true;
    }
    for(int col=0;col<n;col++){
        if(isitsafe(row,col)){
            check[row][col]=1;
        
        if(solvequeen(row+1)){
            return true;
        }
        check[row][col]=0;
    }
    }
    return false;
}
int main() {
   if (solvequeen(0)) {
        for (int i=0; i<n; i++) {
            for (int j=0; j<n; j++)
                cout << check[i][j] << " ";
            cout << endl;
        }
    } else {
        cout << "No solution" << endl;
    }

    return 0;
}
