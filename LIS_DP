#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int arr[n];
    for(int i = 0; i < n; i++)
        cin >> arr[i];

    int dp[n];

    // Step 1: Initialize all LIS lengths to 1
    for(int i = 0; i < n; i++)
        dp[i] = 1;

    // Step 2: Fill DP table
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < i; j++) {
            if(arr[j] < arr[i]) {
                dp[i] = max(dp[i], dp[j] + 1);
            }
        }
    }

    // Print DP Table
    cout << "DP Table: ";
    for(int i = 0; i < n; i++)
        cout << dp[i] << " ";
    cout << endl;

    // Step 3: Find answer
    int ans = 0;
    for(int i = 0; i < n; i++)
        ans = max(ans, dp[i]);

    cout << "LIS length = " << ans;

    return 0;
}
