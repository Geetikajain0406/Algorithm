#include <iostream>
using namespace std;

int fibonacci(int n) {
    // Step 1: Create DP array
    int dp[n+1];

    // Step 2: Base cases
    dp[0] = 0;
    dp[1] = 1;

    // Step 3: Fill the DP table (Bottom-Up)
    for (int i = 2; i <= n; i++) {
        dp[i] = dp[i-1] + dp[i-2];
    }

    // Step 4: Return nth Fibonacci number
    return dp[n];
}

int main() {
    int n;
    cout << "Enter n: ";
    cin >> n;

    cout << "Fibonacci of " << n << " = " << fibonacci(n);
    return 0;
}
