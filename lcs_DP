#include <iostream>
#include<climits>
#include<cstring>
using namespace std;
#define INF 9999
int main(){
    string s1,s2;
   cout<<"enter s1 ";
   cin>>s1;
   cout<<"enter s2 ";
   cin>>s2;
   int dp[50][50];
   int n1=s1.length();
    int n2=s2.length();
    for(int i=0;i<=n1;i++)
    dp[i][0]=0;
     for(int j=0;j<=n2;j++)
    dp[0][j]=0;
   for(int i=1;i<=n1;i++){
       for(int j=1;j<=n2;j++){
           if(s1[i-1]==s2[j-1])
           dp[i][j]=1+dp[i-1][j-1];
           else
            dp[i][j]=max(dp[i-1][j],dp[i][j-1]);
       }
   }
   cout<<"len of lcs is"<<dp[n1][n2];
     cout << "\nDP Table:\n";
    for (int i = 0; i <= n1; i++) {
        for (int j = 0; j <= n2; j++) {
            cout << dp[i][j] << "\t";
        }
        cout << endl;
    }
   
   
    return 0;
}
