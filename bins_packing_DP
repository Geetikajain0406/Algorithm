#include <iostream>
#include<algorithm>
#include<climits>
using namespace std;
#define INF 99999
int main() {
int n=6;
int wt[n];
for(int i=0;i<n;i++){
    cin>>wt[i];
}
int cap=10;
int bincount=0;
int bin[100]={0};
for(int i=0;i<n;i++){
        int j;
        for(j=0;j<bincount;j++){
            if(bin[j]+wt[i]<=cap){
                bin[j]+=wt[i];
                break;
            }
        }
        if(j==bincount){
            bin[bincount]=wt[i];
            bincount++;
        }
    
}
cout<<"number of bins used are:"<<bincount<<endl;
cout<<"the bins are as follow:"<<endl;
for(int i=0;i<bincount;i++){
    cout<<"the bins"<<i+1<<":"<<bin[i]<<endl;
}
return 0;
}
