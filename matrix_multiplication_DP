#include <iostream>
#include<algorithm>
#include<climits>
using namespace std;
#define INF 99999
int main() {
int n=4;
int p[n+1];
for(int i=0;i<=n;i++){
    cin>>p[i];
}
int m[10][10];
for(int i=0;i<=n;i++){
    m[i][i]=0;
}
for(int l=2;l<=n;l++){
    for(int i=1;i<=n-l+1;i++){
        int j=l+i-1;
        m[i][j]=INT_MAX;
        for(int k=i;k<j;k++){
            int cost=m[i][k]+m[k+1][j]+p[i-1]*p[j]*p[k];
            if(cost<m[i][j]){
                m[i][j]=cost;
            }
        }
    }
}
    cout << "\nMinimum number of multiplications = " << m[1][n] << endl;
    cout<<"\n dp table "<<endl;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            if(i<j)
                cout<<m[i][j]<<"\t";
            else
            cout<<"\t";
        }
        cout<<endl;
    }
   return 0;
}
