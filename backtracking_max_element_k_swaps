#include <iostream>
using namespace std;
string ans;
void max(string s,int x){
    if(x==0){
        return;
    }
    int n=s.size();
    for(int i=0;i<n-1;i++){
    for(int j=i+1;j<n;j++){
        if(s[i]<s[j]){
        swap(s[i],s[j]);
        if(ans<s){
            ans=s;
        }
        max(s,x-1);
        swap(s[i],s[j]);
        }
    }
    
    }
}
int main() {
     string M = "254";
    int K = 1;
    ans = M;
    max(M, K);
    cout << ans;
  
    return 0;
}
