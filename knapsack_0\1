#include<iostream>
using namespace std;
int max(int a,int b){
return (a>b)?a:b;
}
int knapsack(int cap,int wt[],int val[],int n){
    int dp[100][100];
for(int i=0;i<=n;i++){
    for(int w=0;w<=cap;w++){
        if(i==0 || w==0)
            dp[i][w]=0;
        else if(wt[i-1]<=w)
            dp[i][w]=max(val[i-1]+dp[i-1][w-wt[i-1]],dp[i-1][w]);
            else
                dp[i][w]=dp[i-1][w];
    }
}
   return dp[n][cap];
}
int main(){
    int n=3;
    int wt[3]={1,2,3};
    int val[3]={60,100,120};
    int cap=5;
    int value=knapsack(cap,wt,val,n);
    cout<<"max value that can be obtained is:"<<value<<endl;
return 0;}
