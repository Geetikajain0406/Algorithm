#include <iostream>
using namespace std;
int main() {
   int n=3;
   int cap=50;
   int wt[]={10,20,30};
   int val[]={60,100,120};
   int dp[n+1][cap+1];
   for(int i=0;i<=n;i++){
       for(int w=0;w<=cap;w++){
           if(w==0||i==0)
           dp[i][w]=0;
           else if(wt[i-1]<=w)
           dp[i][w]=max(dp[i-1][w-wt[i-1]]+val[i-1],dp[i-1][w]);
       
       else
      dp[i][w]= dp[i-1][w];
       }
   }
   cout<<"maximum value:= "<<dp[n][cap]<<endl;
   return 0;
}
