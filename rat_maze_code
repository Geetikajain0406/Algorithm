#include <iostream>
using namespace std;
#define n 4
int sol[n][n];
int maze[n][n] = { {1,0,0,0},
                   {1,1,0,1},
                   {0,1,0,0},
                   {1,1,1,1} };
bool issafe(int x,int y){
    return(x>=0 && y>=0 && x<n && y<n && maze[x][y]==1);
}
bool rat(int x,int y){
    if(x== n-1 && y==n-1){
        sol[x][y]=1;
        return true;
    }
   if(issafe(x,y)){
       sol[x][y]=1;
      if(rat(x+1,y)){
           return true;
       }
        if(rat(x,y+1)){
           return true;
       }
       sol[x][y]=0;
       return false;
   } 
   return false;
}
int main() {
    if(rat(0,0)){
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                cout<<sol[i][j]<<" ";
                
            }
               cout<<endl;
        }
    }
    else{
        cout<<"no solution"<<endl;
    }

    return 0;
}
