#include <iostream>
#include<algorithm>
using namespace std;
int main() {
  int n=5;
  int coins[n]={50,10,2,5,1};
  int value=63;
  for(int i=0;i<n-1;i++){
      for(int j=0;j<n-i-1;j++){
          if(coins[j] < coins[j + 1]){
             int temp = coins[j];
                coins[j] = coins[j + 1];
                coins[j + 1] = temp;
          }
      }
  }
  cout << "Coins used: ";
  for(int i=0;i<n;i++){
      while(value>=coins[i]){
          cout<<coins[i]<<" ";
          value-=coins[i];
      }
  }
  if(value!=0){
      cout<<"no soln"<<endl;
  }
  else{
      
        cout << "\nChange successfully made!";
  }
  return 0;
}
